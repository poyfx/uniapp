<view class="content"><view class="step-box"><step class="vue-ref" vue-id="1" value="{{step.value1}}" actives="{{step.active}}" data-ref="one" bind:__l="__l"></step><step class="vue-ref" vue-id="2" value="{{step.value2}}" actives="{{step.kong}}" data-ref="two" bind:__l="__l"></step><step class="vue-ref" vue-id="3" value="{{step.value3}}" actives="{{step.kong1}}" data-ref="three" bind:__l="__l"></step></view><form enctype="multipart/form-data" data-event-opts="{{[['reset',[['formReset',['$event']]]]]}}" bindreset="__e"><view class="fget-num"><view class="fget-ul"><block wx:if="{{num<2}}"><text>上传身份证<text style="color:#FF0000;">{{info[num]}}</text></text></block><block wx:else><text>上传<text style="color:#FF0000;">{{info[num]}}</text>授权委托书</text></block><text data-event-opts="{{[['tap',[['all',['$event']]]]]}}" class="color-dff" bindtap="__e">点击上传</text></view><view class="positive"><image class="idCard" src="{{src}}" mode="aspectFit"></image></view><block wx:if="{{num<2}}"><view data-event-opts="{{[['tap',[['previewImage',['$0'],['num']]]]]}}" class="tellinfo" bindtap="__e"><text>{{tellinfo[num]}}</text></view></block><block wx:else><view data-event-opts="{{[['tap',[['previewImage',['$0'],['num']]]]]}}" class="tellinfo tellinfos" bindtap="__e"><text>{{tellinfo[num]}}</text></view></block><view hidden="{{!(btn.stepThree)}}" class="flex  m-info"><view class="flex center m-info-content"><text>授权书有效期</text><input hidden="{{!(times)}}" style="width:222px;height:37.5px;" placeholder="请选择,需与授权书的有效日期一致" disabled="true" data-event-opts="{{[['tap',[['changeTimes',['$event']]]],['input',[['__set_model',['','inputs','$event',[]]]]]]}}" value="{{inputs}}" bindtap="__e" bindinput="__e"/><view hidden="{{!(!times)}}" style="width:178px;"><view hidden="{{!(!times)}}"><view data-event-opts="{{[['tap',[['changeTimes',['$event']]]]]}}" bindtap="__e">{{''+day+''}}</view></view><w-picker class="vue-ref" vue-id="4" mode="date" startYear="2019" endYear="2030" defaultVal="{{[0,1,3]}}" current="{{true}}" themeColor="#00A8FF" data-ref="date" data-event-opts="{{[['^confirm',[['onConfirm']]]]}}" bind:confirm="__e" bind:__l="__l"></w-picker></view></view><view class="flex m-info-text"><image style="width:12px;height:12px;" src="../../../static/img/right.png" mode="aspectFit"></image></view></view></view></form><view hidden="{{!(btn.stepOne)}}" class="mTop30 mB"><m-button vue-id="5" type="{{btn.type}}" value="{{btn.value}}" data-event-opts="{{[['^oneSide',[['oneSide']]]]}}" bind:oneSide="__e" bind:__l="__l"></m-button></view><view hidden="{{!(btn.stepTwo)}}" class="m-two-btn mTop30 mB"><t-button class="tButton" vue-id="6" type="{{btn.type}}" disabled="{{btn.disabled}}" content="{{btn.content}}" data-event-opts="{{[['^lastStep',[['lastStep']]]]}}" bind:lastStep="__e" bind:__l="__l"></t-button><t-button class="tButton" vue-id="7" type="{{btn.type}}" disabled="{{btn.disabled}}" content="{{btn.value}}" data-event-opts="{{[['^nextStep',[['nextStep']]]]}}" bind:nextStep="__e" bind:__l="__l"></t-button></view><view hidden="{{!(btn.stepThree)}}" class="m-two-btn mTop30 mB"><t-button class="tButton" vue-id="8" type="{{btn.type}}" disabled="{{btn.disabled}}" content="{{btn.content}}" data-event-opts="{{[['^threeStepLast',[['threeStepLast']]]]}}" bind:threeStepLast="__e" bind:__l="__l"></t-button><t-button class="tButton" vue-id="9" type="{{btn.type}}" disabled="{{btn.disabled}}" content="{{btn.commit}}" data-event-opts="{{[['^threeStepNext',[['threeStepNext']]]]}}" bind:threeStepNext="__e" bind:__l="__l"></t-button></view></view>