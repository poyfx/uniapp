<view><view class="chooseAddress"><view class="status_bar"></view><view class="flex title"><image src="../../../static/img/back.png" mode="aspectFit" data-event-opts="{{[['tap',[['back',['$event']]]]]}}" bindtap="__e"></image><view>预约详情</view><view>提油码</view></view><view><block wx:if="{{status==1||status==2||status==3||status==4||status==5}}"><view class="step-box"><step vue-id="1" value="{{step.value1}}" actives="{{step.waitSure}}" bind:__l="__l"></step><step vue-id="2" value="{{step.value2}}" actives="{{step.waitSend}}" bind:__l="__l"></step><step vue-id="3" value="{{step.value3}}" actives="{{step.waitGet}}" bind:__l="__l"></step></view></block><block wx:if="{{status==-1}}"><view class="step-box"><step vue-id="4" value="{{step.value4}}" actives="{{step.refuse}}" bind:__l="__l"></step></view></block><block wx:if="{{status==9}}"><view class="step-box"><step vue-id="5" value="{{step.value5}}" actives="{{step.complete}}" bind:__l="__l"></step></view></block><view class="content"><view class="fget-num paddingLeft15"><info-text bind:input="__e" vue-id="6" textValue="{{confirmed.order}}" disabled="{{disabled}}" value="{{order}}" data-event-opts="{{[['^input',[['__set_model',['','order','$event',[]]]]]]}}" bind:__l="__l"></info-text><info-text bind:input="__e" vue-id="7" textValue="{{confirmed.time}}" disabled="{{disabled}}" value="{{time}}" data-event-opts="{{[['^input',[['__set_model',['','time','$event',[]]]]]]}}" bind:__l="__l"></info-text><info-text bind:input="__e" vue-id="8" textValue="{{confirmed.company}}" disabled="{{disabled}}" value="{{company}}" data-event-opts="{{[['^input',[['__set_model',['','company','$event',[]]]]]]}}" bind:__l="__l"></info-text><info-text bind:input="__e" vue-id="9" textValue="{{confirmed.oil}}" disabled="{{disabled}}" value="{{oil}}" data-event-opts="{{[['^input',[['__set_model',['','oil','$event',[]]]]]]}}" bind:__l="__l"></info-text><info-text bind:input="__e" vue-id="10" textValue="{{confirmed.much}}" disabled="{{disabled}}" value="{{much}}" data-event-opts="{{[['^input',[['__set_model',['','much','$event',[]]]]]]}}" bind:__l="__l"></info-text><info-text bind:input="__e" vue-id="11" textValue="{{confirmed.main}}" disabled="{{disabled}}" value="{{main}}" data-event-opts="{{[['^input',[['__set_model',['','main','$event',[]]]]]]}}" bind:__l="__l"></info-text><view hidden="{{!(showAddress)}}" class="fget-eara addressimg"><view class="first-li">送油地址：</view><view>{{address}}</view></view></view><view class="nextBox"><m-button vue-id="12" type="{{btn.type}}" value="{{btn.value}}" data-event-opts="{{[['^close',[['close']]]]}}" bind:close="__e" bind:__l="__l"></m-button></view></view></view></view></view>