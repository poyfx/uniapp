      "compilerVersion": "2.4.2",
      "control": "v8",
      "nvueCompiler": "uni-app",
      "renderer": "auto",
      "nvue": {
        "flex-direction": "column"
      }
    },
    "tabBar": {
      "color": "#000000",
      "selectedColor": "#009DFF",
