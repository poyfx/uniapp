          "<uses-permission android:name=\"android.permission.WRITE_EXTERNAL_STORAGE\"/>",
          "<uses-permission android:name=\"android.permission.WRITE_SETTINGS\"/>"
        ]
      },
      "apple": {},
      "plugins": {
        "oauth": {},
        "speech": {},
        "push": {
          "unipush": {}
        },
        "audio": {
          "mp3": {
            "description": "Android平台录音支持MP3格式文件"
          }
        }
      }
    },
    "safearea": {
      "background": "#e5e5e5",
      "bottom": {
        "offset": "none"
      },
      "left": {
        "offset": "none"
      },
      "right": {
        "offset": "none"
      }
    },
    "arguments": "{\"name\":\"\",\"path\":\"\",\"query\":\"\",\"id\":0}",
