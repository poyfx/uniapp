<view class="content"><view class="step-box"><step class="vue-ref" vue-id="1" value="{{step.value1}}" actives="{{step.active}}" data-ref="one" bind:__l="__l"></step><step class="vue-ref" vue-id="2" value="{{step.value2}}" actives="{{step.kong}}" data-ref="two" bind:__l="__l"></step><step class="vue-ref" vue-id="3" value="{{step.value3}}" actives="{{step.kong1}}" data-ref="three" bind:__l="__l"></step></view><form enctype="multipart/form-data" data-event-opts="{{[['reset',[['formReset',['$event']]]]]}}" bindreset="__e"><view class="fget-num"><view class="fget-ul"><text>{{info[num]}}</text><text data-event-opts="{{[['tap',[['all',['$event']]]]]}}" class="color-dff" bindtap="__e">点击上传</text></view><view class="positive"><image class="idCard" src="{{src}}" mode="aspectFit"></image></view><view class="tellinfo"><text>{{tellinfo[num]}}</text></view><view hidden="{{!(btn.stepThree)}}" class="flex  m-info"><view class="flex center m-info-content"><text>授权书有效期</text><input hidden="{{!(times)}}" style="width:222px;height:37.5px;" placeholder="请选择,需与授权书的有效日期一致" disabled="true" data-event-opts="{{[['tap',[['changeTimes',['$event']]]]]}}" bindtap="__e"/><view hidden="{{!(!times)}}"><rui-date-picker hidden="{{!(!times)}}" class="day" vue-id="4" fields="day" start="2010-00-00" end="2030-12-31" value="{{day}}" data-event-opts="{{[['^change',[['bindChange']]]]}}" bind:change="__e" bind:__l="__l"></rui-date-picker></view></view><view class="flex m-info-text"><image style="width:12px;height:12px;" src="../../../static/img/right.png" mode="aspectFit"></image></view></view></view></form><view hidden="{{!(btn.stepOne)}}" class="mTop30"><m-button vue-id="5" type="{{btn.type}}" value="{{btn.value}}" data-event-opts="{{[['^oneSide',[['oneSide']]]]}}" bind:oneSide="__e" bind:__l="__l"></m-button></view><view hidden="{{!(btn.stepTwo)}}" class="m-two-btn mTop30"><t-button class="tButton" vue-id="6" type="{{btn.type}}" disabled="{{btn.disabled}}" content="{{btn.content}}" data-event-opts="{{[['^lastStep',[['lastStep']]]]}}" bind:lastStep="__e" bind:__l="__l"></t-button><t-button class="tButton" vue-id="7" type="{{btn.type}}" disabled="{{btn.disabled}}" content="{{btn.value}}" data-event-opts="{{[['^nextStep',[['nextStep']]]]}}" bind:nextStep="__e" bind:__l="__l"></t-button></view><view hidden="{{!(btn.stepThree)}}" class="m-two-btn mTop30"><t-button class="tButton" vue-id="8" type="{{btn.type}}" disabled="{{btn.disabled}}" content="{{btn.content}}" data-event-opts="{{[['^threeStepLast',[['threeStepLast']]]]}}" bind:threeStepLast="__e" bind:__l="__l"></t-button><t-button class="tButton" vue-id="9" type="{{btn.type}}" disabled="{{btn.disabled}}" content="{{btn.commit}}" data-event-opts="{{[['^threeStepNext',[['threeStepNext']]]]}}" bind:threeStepNext="__e" bind:__l="__l"></t-button></view></view>